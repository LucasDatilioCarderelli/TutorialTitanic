   "source": [
    "def SexToBin(valor):\n",
    "    if valor == 'female':\n",
    "        return 1\n",
    "    else:\n",
    "        return 0\n",
    "    \n",
    "df_train['Sex_Bin'] = df_train['Sex'].map(SexToBin)\n",
    "\n",
    "df_train.head()"
   ]
